<% form_for @data_object do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>

    <%= f.hidden_field(:data_type_id, :value => params[:data_type_id]) %><br />

  <p><%= f.submit "Submit" %></p>
<% end %>
