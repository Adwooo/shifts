<% title "Data Objects" %>

<% form_tag '/data_objects/index' do %>
  Group by 
  <%= select_tag "view_options[group_type]", options_for_select(@group_type_options, @selected_type) %>
  &nbsp;&nbsp;&nbsp;&nbsp;
  <%= select_tag "view_options[group_by]", options_for_select(@group_by_options, @selected_by) %>
  <%= submit_tag "Update" %>
<% end -%>

<%= render(:partial => "data_objects_table" , :collection => @data_types) %>
<%#= render :partial => "data_objects_table" %>




<h1> DEPRECATED </h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>
  <% for data_object in @data_objects %>
    <tr>
      <td><%=h data_object.name %></td>
      <td><%=h data_object.description %></td>
      <td><%= link_to "Show", data_object %></td>
      <td><%= link_to "Edit", edit_data_object_path(data_object) %></td>
      <td><%= link_to "Destroy", data_object, :confirm => 'Are you sure?', 
                                              :method => :delete %></td>
      <td><%= link_to "View Entries", data_object_data_entries_path(data_object) %></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Data Object", new_data_type_data_object_path %>&nbsp;&nbsp;
<%= link_to "Back to Data Types List", data_types_path %></p>
