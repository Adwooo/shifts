    <% if @user_profile_entries.empty? %>
    <strong><h3> This user does not have any publicly viewable information for this department </h3></strong>
    <% else %>
      <% for entry in @user_profile_entries %>
      <p>
        <strong><%=h "#{entry.field_name}: " %></strong>
        <%=h entry.content %>
      </p>
      <% end %>
    <% end %>

