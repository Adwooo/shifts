<!-- LOOP THROUGH PRE-DETERMINED ROWS HERE -->
<ul class="event-row">
  <%= render :partial => "/templates/schedule/event", :collection => @week_template.requested_shifts.on_day(day).assigned(event_group) %>
</ul>

