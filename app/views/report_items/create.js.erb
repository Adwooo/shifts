setTimeout(function(){
	var height = $('#dashboard_report_view')[0].scrollHeight
	$('#all_report_items').html("<%= escape_javascript render partial: 'reports/report_item', collection: @report.report_items  %>");
	$('#dashboard_report_view').scrollTop(height);
	$('#current_report_item td').effect("highlight", 1100);	
},3000)
